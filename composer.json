{
    "name": "greatplr/amember-sso",
    "description": "Laravel package for aMember SSO authentication, webhook handling, and access control",
    "type": "library",
    "keywords": ["laravel", "amember", "sso", "authentication", "subscription"],
    "license": "MIT",
    "authors": [
        {
            "name": "GreatPLR",
            "email": "support@greatplr.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "plutuss/amember-pro-laravel": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Greatplr\\AmemberSso\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greatplr\\AmemberSso\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Greatplr\\AmemberSso\\AmemberSsoServiceProvider"
            ],
            "aliases": {
                "AmemberSso": "Greatplr\\AmemberSso\\Facades\\AmemberSso"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
